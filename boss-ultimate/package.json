    "react-number-format": "^3.3.4",
    "react-popper": "^0.10.4",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scroll-parallax": "^1.3.5",
    "react-scrollspy": "^3.3.5",
    "react-select": "^1.2.1",
    "react-slick": "^0.23.1",

    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0",
    "reselect": "^3.0.1",
    "sanitize.css": "4.1.0",
    "serve-favicon": "^2.4.5",
    "slick-carousel": "^1.8.1",
    "warning": "4.0.1",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {

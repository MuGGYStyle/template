    "material-ui-pickers": "2.0.2",
    "minimist": "1.2.0",
    "mui-datatables": "^2.0.0-beta-1",
    "prop-types": "15.6.1",
    "rcolor": "^1.0.1",
    "react": "16.4.1",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-animated-slider": "^1.1.2",
    "react-autosuggest": "^9.3.4",
    "react-big-calendar": "^0.19.1",
